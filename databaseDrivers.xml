<application>
  <component name="LocalDatabaseDriverManager" version="201">
    <driver id="mysql.8">
      <url-template name="default" template="jdbc:mysql://{host::localhost}?[:{port::3306}][/{database}?][\?&lt;&amp;,user={user},password={password},{:identifier}={:param}&gt;]" disable="true" />
      <url-template name="Unix Socket" template="jdbc:mysql:///{database}?[\?&lt;&amp;,user={user},password={password},junixsocket.file={mysql.socket::/tmp/mysqld.sock},socketFactory={socketFactory:#param:org.newsclub.net.mysql.AFUNIXDatabaseSocketFactoryCJ},{:identifier}={:param}&gt;]" disable="true" />
      <url-template name="default" template="jdbc:mysql://address=\(protocol=tcp\)\(&lt;\)\(,host={host:host_ipv6:localhost},port={port::3306},user={user},password={password},{:identifier}={:param}&gt;\)[/{database}][\?&lt;&amp;,{:identifier}={:param}&gt;]" disable="true" />
      <url-template name="default" template="jdbc:mysql://{host::localhost}?[:{port::3306}][/{database}?][\?&lt;&amp;,user={user},password={password},{:identifier}={:identifier}&gt;]" />
      <url-template name="default" template="jdbc:mysql://address=\(protocol=tcp\)\(&lt;\)\(,host={host:host_ipv6:localhost},port={port::3306},user={user},password={password},{:identifier}={:identifier}&gt;\)[/{database}][\?&lt;&amp;,{:identifier}={:param}&gt;]" />
      <url-template name="new" template="jdbc:mysql://:3306" />
      <artifact />
      <artifact id="MySQL ConnectorJ" name="MySQL ConnectorJ" channel="fixed" rolling="false" use="true" version="8.0.21" />
      <property name="characterEncoding" value="" />
    </driver>
    <driver id="mysql">
      <artifact />
      <artifact id="MySQL ConnectorJ" name="MySQL ConnectorJ" use="true" version="5.1.47" />
    </driver>
  </component>
</application>